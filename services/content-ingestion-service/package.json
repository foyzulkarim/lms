{
  "name": "content-ingestion-service",
  "version": "1.0.0",
  "description": "LMS Content Ingestion Service - Processes educational content, extracts text, generates embeddings",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "tsx watch src/main.ts",
    "worker:dev": "tsx watch workers/extraction-worker.ts",
    "worker:extraction": "node dist/workers/extraction-worker.js",
    "worker:embedding": "node dist/workers/embedding-worker.js",
    "worker:indexing": "node dist/workers/indexing-worker.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "db:migrate": "node dist/database/migrate.js",
    "db:seed": "node dist/database/seed.js"
  },
  "keywords": [
    "lms",
    "content-ingestion",
    "text-extraction",
    "embeddings",
    "vector-database",
    "ocr",
    "speech-to-text",
    "document-processing"
  ],
  "author": "LMS Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.0.0",
    "@fastify/multipart": "^8.0.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.0",
    "@opentelemetry/exporter-jaeger": "^1.17.0",
    "@opentelemetry/sdk-node": "^0.45.0",
    "bullmq": "^4.15.0",
    "cheerio": "^1.0.0-rc.12",
    "fastify": "^4.24.3",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "pg": "^8.11.3",
    "mongodb": "^6.3.0",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "zod": "^3.22.4",
    "uuid": "^9.0.1",
    "mime-types": "^2.1.35",
    "file-type": "^18.7.0",
    "sharp": "^0.32.6",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "xlsx": "^0.18.5",
    "node-html-parser": "^6.1.11",
    "turndown": "^7.1.2",
    "axios": "^1.6.2",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "ytdl-core": "^4.11.5",
    "simple-git": "^3.20.0",
    "archiver": "^6.0.1",
    "unzipper": "^0.10.14",
    "node-cron": "^3.0.3",
    "prom-client": "^15.0.0",
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.7",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.11",
    "@types/archiver": "^6.0.2",
    "@types/unzipper": "^0.10.9",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.4.0",
    "typescript": "^5.2.2",
    "prettier": "^3.1.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/lms-content-ingestion-service.git"
  },
