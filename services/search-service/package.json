{
  "name": "@lms/search-service",
  "version": "2.0.0",
  "description": "Advanced Search Service for LMS - RAG-enabled semantic search, full-text search, and AI-powered content discovery",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t lms/search-service .",
    "docker:run": "docker run -p 3008:3008 --env-file .env lms/search-service",
    "index:create": "tsx scripts/create-indices.ts",
    "index:reindex": "tsx scripts/reindex-all.ts",
    "index:delete": "tsx scripts/delete-indices.ts",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback"
  },
  "keywords": [
    "lms",
    "search",
    "rag",
    "semantic-search",
    "elasticsearch",
    "vector-search",
    "ai-powered",
    "microservice"
  ],
  "author": "LMS Team",
  "license": "MIT",
