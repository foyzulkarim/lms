    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@elastic/elasticsearch": "^8.11.0",
    "axios": "^1.6.2",
    "compromise": "^14.10.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "lodash": "^4.17.21",
    "lru-cache": "^10.0.1",
    "natural": "^6.12.0",
    "opossum": "^8.0.0",
    "pg": "^8.11.3",
    "pgvector": "^0.1.8",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "stopword": "^2.0.8",
    "tiktoken": "^1.0.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
