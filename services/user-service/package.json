{
  "name": "user-service",
  "version": "1.0.0",
  "description": "User Service for LMS - Manages user profiles, enrollments, and preferences",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "tsx watch src/app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:make": "knex migrate:make",
    "seed": "knex seed:run",
    "db:reset": "npm run migrate:rollback && npm run migrate && npm run seed"
  },
  "keywords": [
    "user-service",
    "lms",
    "fastify",
    "typescript",
    "postgresql",
    "redis",
    "rabbitmq"
  ],
  "author": "LMS Team",
  "license": "MIT",
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.0.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@fastify/multipart": "^8.0.0",
    "@sinclair/typebox": "^0.31.28",
    "knex": "^3.0.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "amqplib": "^0.10.3",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.6.0",
    "lodash": "^4.17.21",
    "date-fns": "^2.30.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/pg": "^8.10.7",
    "@types/amqplib": "^0.10.4",
    "@types/uuid": "^9.0.6",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.200",
    "@types/jest": "^29.5.7",
    "@types/supertest": "^2.0.15",
    "typescript": "^5.2.2",
    "tsx": "^4.1.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
