{
  "name": "@lms/analytics-service",
  "version": "1.0.0",
  "description": "Learning Management System Analytics Service - Real-time analytics, reporting, and business intelligence",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "nodemon --exec ts-node src/main.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "docker:build": "docker build -t lms-analytics-service .",
    "docker:run": "docker run -p 3007:3007 lms-analytics-service"
  },
  "keywords": [
    "lms",
    "analytics",
    "learning-analytics",
    "reporting",
    "business-intelligence",
    "education-technology",
    "data-analytics",
    "performance-tracking"
  ],
  "author": "LMS Team",
  "license": "MIT",
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.2",
    "@fastify/helmet": "^11.1.1",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@fastify/rate-limit": "^8.1.0",
    "@fastify/jwt": "^7.2.4",
    "pg": "^8.11.3",
    "knex": "^3.0.1",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "bullmq": "^4.15.0",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "prom-client": "^15.0.0",
    "csv-writer": "^1.6.0",
    "xlsx": "^0.18.5",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
