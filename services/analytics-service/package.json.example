{
  "name": "lms-analytics-service",
  "version": "1.0.0",
  "description": "Real-time learning analytics and reporting service for LMS",
  "main": "dist/main.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "worker:dev": "nodemon --exec ts-node src/worker.ts",
    "worker:start": "node dist/worker.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "migrate": "node dist/migrations/migrate.js",
    "seed": "node dist/seeds/seed.js",
    "docker:build": "docker build -t lms/analytics-service .",
    "docker:build:worker": "docker build -f Dockerfile.worker -t lms/analytics-worker ."
  },
  "keywords": [
    "lms",
    "analytics",
    "reporting",
    "microservices",
    "learning-management"
  ],
  "author": "LMS Team",
  "license": "MIT",
  "dependencies": {
    "fastify": "^4.24.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.0.0",
    "@fastify/rate-limit": "^8.0.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "kafkajs": "^2.2.4",
    "bull": "^4.12.0",
    "bullmq": "^4.15.0",
    "zod": "^3.22.0",
    "winston": "^3.11.0",
    "prom-client": "^15.0.0",
    "csv-writer": "^1.6.0",
    "xlsx": "^0.18.5",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.0",
    "canvas": "^2.11.2",
    "chart.js": "^4.4.0",
    "chartjs-node-canvas": "^4.1.6",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/sdk-node": "^0.45.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.0",
    "@opentelemetry/exporter-jaeger": "^1.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.0",
    "@types/lodash": "^4.14.0",
    "@types/uuid": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/nodemailer": "^6.4.0",
    "typescript": "^5.2.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "supertest": "^6.3.0",
    "@types/supertest": "^2.0.0",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/lms-analytics-service.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/lms-analytics-service/issues"
  },
  "homepage": "https://github.com/your-org/lms-analytics-service#readme"
}
